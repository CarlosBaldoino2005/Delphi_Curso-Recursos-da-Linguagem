unit View.Calculadora.Somas;

interface

uses
  View.Calculadora.Interfaces;

type
  TCalculadoraSomas = class(TInterfacedObject, iCalculadoraOperacoes)
    private
    public
      function Calcular ( A, B : Integer ) : Integer;
  end;

implementation

{ TCalculadoraSomas }

function TCalculadoraSomas.Calcular(A, B: Integer): Integer;
begin
  Result := A + B;
end;

end.
